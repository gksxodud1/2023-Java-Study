package Week2;


    /* 다른 프로젝트에 있는 필드를 참조한 후 자신의 대학교 정보를 출력하는 코드
만약 졸업을 했을 경우는 학년에 조교라고 작성!
 */


    public class wkdeogus2 {
        public static void main(String[] args) {
            wkdeogus1 w1 = new wkdeogus1("삼육보건대학교", "의료정보과", "2학년");

            System.out.println("대학교 이름은 : " + w1.university);
            System.out.println("학과는 : " + w1.department);
            System.out.println("학년은 : " + w1.schoolYear);
        }
}
